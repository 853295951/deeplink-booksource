    "version": 100,
    "search": {
        "url": "http://yunduwu.com/novel/search@post->keywords=${key}",
         "charset": "UTF-8",
        "list": "article > a",
        "name": "h3",
        "author": ".list-sumary> span:nth-child(1)@match->(?<=作者：).+",
        "cover": ".list-img>img",
        "detail": "a"
    },
    "detail": {
        "summary":"meta[property=og:description]@attr->content",
        "status": "meta[property=og:novel:status]@attr->content",
        "update": "meta[property=og:novel:update_time]@attr->content",
        "lastChapter": "meta[property=og:novel:latest_chapter_name]@attr->content",
        "catalog": "a.btn.btn-warning.large"
    },
    "catalog": {
        "list": ".contents>ul > li",
        "page":"body > section > div > div:nth-child(5) > ul > li:nth-child(4) > a",
        "name": "a",
        "chapter": "a"
    },
    "chapter": {
        "content":".TXT>DIV"
    }
}
